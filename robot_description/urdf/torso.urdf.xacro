<?xml version="1.0" ?>
<robot  xmlns:xacro="http://ros.org/wiki/xacro">

<!--<xacro:include filename="$(find robot_description)/urdf/ur10.gazebo.xacro" />-->
<xacro:include filename="$(find robot_description)/urdf/torso.transmission.xacro" />



<xacro:macro name="cylinder_inertial" params="radius length mass *origin">
    <inertial>
      <mass value="${mass}" />
      <insert_block name="origin" />
      <inertia ixx="${0.0833333 * mass * (3 * radius * radius + length * length)}" ixy="0.0" ixz="0.0"
        iyy="${0.0833333 * mass * (3 * radius * radius + length * length)}" iyz="0.0"
        izz="${0.5 * mass * radius * radius}" />
    </inertial>
  </xacro:macro>


	<xacro:macro name="torso" params="name prefix">



		<!-- link list -->
		<link name="base_link">
			<visual>
<origin rpy="0 0 0" xyz="0 0 0"/>
				<geometry>
					<mesh filename="package://robot_description/meshes/visual/motoman_base.stl" />
				</geometry>
				<material name="blue">
					<color rgba="0 0 1 1"/>
				</material>
			</visual>
			<collision>
<origin rpy="0 0 0" xyz="0 0 0"/>
				<geometry>
					<mesh filename="package://robot_description/meshes/collision/motoman_base.stl" />
				</geometry>
				<material name="yellow">
					<color rgba="0 1 1 1"/>
				</material>
			</collision>
<xacro:cylinder_inertial radius="0.5" length="0.5" mass="15">
        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
      </xacro:cylinder_inertial>
		</link>
		<link name="${prefix}link_b1">
			<visual>
				<origin rpy="0 0 0" xyz="0 0 0"/>
				<geometry>
					<mesh filename="package://robot_description/meshes/visual/motoman_axis_b1.stl"/>
				</geometry>
				<material name="silver">
