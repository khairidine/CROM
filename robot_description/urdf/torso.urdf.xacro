				</geometry>
				<material name="yellow"/>
			</collision>
<xacro:cylinder_inertial radius="0.51" length="0.51" mass="15">
        <origin xyz="0.0 0.0 0.0" rpy="0 0 0" />
      </xacro:cylinder_inertial>
		</link>
